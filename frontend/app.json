{
  "expo": {
    "name": "Dokubox",
    "slug": "dokubox",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dokubox.app"
    },
    "android": {
      "package": "com.dokubox.app",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to upload documents."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "The app needs access to your camera to take photos of documents."
        }
      ]
    ]
  }
}

